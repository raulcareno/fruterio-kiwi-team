  <Dimension type="StandardDimension" highCardinality="false" name="MateriaPrima" caption="Materia Prima" description="Utilizada en el Cubo de Disponibilidad de Materia Prima">
    <Hierarchy name="MateriaPrima" hasAll="true" allMemberName="Todas las Materias Primas" primaryKey="id" caption="Materia Prima">
      <Table name="disponibilidadmateriaprima">
