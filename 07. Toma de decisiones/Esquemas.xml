  <Dimension type="StandardDimension" highCardinality="false" name="Modelo" caption="Modelos" description="Utilizada en el Cubo de Disponibilidad de Productos">
    <Hierarchy name="Modelo" hasAll="true" allMemberName="Todos los Modelos" primaryKey="id" caption="Modelos">
      <Table name="modelo">
      </Table>
      <Level name="Modelo" column="nombre" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never" captionColumn="nombre">
      </Level>
    </Hierarchy>
  </Dimension>
  <Dimension type="StandardDimension" highCardinality="false" name="NumeroProducto" caption="Numero de Producto" description="Utilizada en el Cubo de Disponibilidad de Productos">
    <Hierarchy name="NumeroProducto" hasAll="true" allMemberName="Todos los Productos" primaryKey="id" caption="Numero de Producto">
      <Table name="disponibilidadproductos">
      </Table>
      <Level name="NumeroProducto" column="nroProducto" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never" captionColumn="nroProducto">
      </Level>
    </Hierarchy>
