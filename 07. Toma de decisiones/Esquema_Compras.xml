<Schema name="Compra">
  <Dimension type="StandardDimension" highCardinality="false" name="Divisa">
    <Hierarchy name="Divisa" hasAll="true" primaryKey="id">
      <Table name="divisa">
      </Table>
      <Level name="NombreDivisa" column="nombre" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
    </Hierarchy>
  </Dimension>
  <Dimension type="StandardDimension" highCardinality="false" name="Tiempo">
    <Hierarchy name="Tiempo" hasAll="true" primaryKey="id">
      <Table name="tiempo">
      </Table>
      <Level name="Anio" column="a&#241;o" type="Integer" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="Trimestre" column="trimestre" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="Mes" column="mes_desc" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="Dia" column="dia_de_la_semana_desc" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
    </Hierarchy>
  </Dimension>
  <Dimension type="StandardDimension" highCardinality="false" name="MateriaPrimaProducto">
    <Hierarchy name="Materia prima - Producto" hasAll="true" primaryKey="id">
      <Table name="materiaprimaproducto">
      </Table>
      <Level name="Nombre" column="nombre" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="Tipo" column="tipo" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="Codigo" column="codigo" type="Integer" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="FechaIngreso" column="fecha" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
    </Hierarchy>
  </Dimension>
  <Dimension type="StandardDimension" highCardinality="false" name="CompraProveedor">
    <Hierarchy name="Proveedor" hasAll="true" primaryKey="id">
      <Table name="compras">
      </Table>
      <Level name="NombreProveedor" column="nombreProveedor" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
      <Level name="MailProveedor" column="mailProveedor" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
      </Level>
    </Hierarchy>
  </Dimension>
  <Cube name="Compra" cache="true" enabled="true">
    <Table name="compras">
    </Table>
    <DimensionUsage source="Divisa" name="Divisa" foreignKey="idDivisa" highCardinality="false">
    </DimensionUsage>
    <DimensionUsage source="Tiempo" name="Tiempo" foreignKey="idTiempo" highCardinality="false">
    </DimensionUsage>
    <DimensionUsage source="MateriaPrimaProducto" name="MateriaPrimaProducto" foreignKey="idMateriaPrimaProducto" highCardinality="false">
    </DimensionUsage>
    <DimensionUsage source="CompraProveedor" name="Proveedor" foreignKey="id" highCardinality="false">
    </DimensionUsage>
    <Measure name="Cantidad Comprada" column="cantidadComprada" datatype="Integer" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Cantidad Pagos" column="cantidadPagos" datatype="Integer" aggregator="count" visible="true">
    </Measure>
    <Measure name="Cantidad Prod. Defectuosos" column="cantidadProductosDefectuosos" datatype="Integer" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Importe" column="importe" datatype="Numeric" aggregator="sum" visible="true">
    </Measure>
  </Cube>
</Schema>
