      </Level>
      <Level name="Mes" column="mes_desc" type="String" uniqueMembers="true" levelType="TimeMonths" hideMemberIf="Never" captionColumn="mes_desc">
      </Level>
      <Level name="Dia" column="dia_de_la_semana_completa" type="String" uniqueMembers="true" levelType="TimeDays" hideMemberIf="Never" captionColumn="dia_de_la_semana_completa">
